<div id="header">
  <%=t '.headline' %>
</div>

<%= form_tag binaries_url, { :multipart => true } do |f| %>

  <div id="binary">
    <%= file_field_tag :upload %>
  </div>

  <div id="footer">
    <div class="menu_two_items">
      <div class="menu_item">
        <%= image_submit_tag("binary.png", :height => 61) %><br/>
        <%=t '.submit' %>
      </div>
      <div class="menu_item">
        <%= link_to image_tag('trash.png', :height => 61), root_url %><br/>
        <%=t 'cancel' %>
      </div>
    </div>
  </div>
  <div class="clear"></div>

<% end %>

<%= info_block %>
